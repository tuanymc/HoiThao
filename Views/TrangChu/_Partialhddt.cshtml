@model IEnumerable<KhoaXayDung.Models.Tin>

@*<div class="row" style="margin:20px 0 40px 0;">*@
    <h1 class="chumong">HOẠT ĐỘNG CỦA GIẢNG VIÊN VÀ SINH VIÊN</h1>
    
    @foreach (var item in Model)
    {
        <div class="col-sm-9 col-sm-3 col-md-3">
            <div class="thumbnail">
                <a style="text-decoration:none;" href="@Url.Action("Index", "TinTuc", new { id = item.MaT })">
                    <img src="~/img/ckeditor/Images/@item.H" alt="...">
                    <div class="caption">
                        <h3 class="chumong">@item.TenT</h3>

                    </div>
                </a>
            </div>
        </div>
    }

    
@*</div>*@
