@model IEnumerable <KhoaXayDung.Models.QCLink>

@if (Model.Count() > 0)
{
    <h4 class="chumong">LIÊN KẾT WEBSITE</h4>
    <div id="marquee" class="marquee" style="width: 100%;overflow: hidden;">
        @foreach (var item in Model)
        {
            <a target="_blank" href="@item.Link">
                <img src="~/img/ckeditor/Images/@item.H" alt="@item.TenQC" />
            </a>
        }
    </div>
}
