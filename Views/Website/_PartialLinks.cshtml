@model IEnumerable<KhoaXayDung.Models.Tin>

@if (Model.Count() > 0)
{
    <div class="container qc-trung-tam">
        <div class="trung-tam row">
            @foreach (var item in Model)
            {
                <div class="col-xs-3">
                    <a href="/danh-muc/@item.LoaiTin.khongdau/@item.Link"><img src="~/img/ckeditor/Images/@item.H?width=273&amp;height=250&amp;mode=crop&amp;anchor=middle" class="img-responsive"></a>
                </div>
            }
        </div>
    </div>
}
else
{
    Layout = null;
}

